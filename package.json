{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@9.12.3",
  "scripts": {
    "dev": "turbo dev --filter=web",
    "build": "turbo build",
    "start": "pnpm --filter web start",
    "lint": "turbo lint",
    "lint:fix": "turbo lint -- --fix",
    "typecheck": "turbo typecheck",
    "test": "turbo test",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm exec eslint --max-warnings=0 --fix"
    ],
    "*.{js,jsx,ts,tsx,md,css,json}": [
      "pnpm exec prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@playwright/test": "^1.48.0",
    "@types/node": "^20",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
    "eslint-config-next": "^14.2.11",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "prettier": "^3.6.2",
    "prisma": "^5.21.1",
    "tailwindcss": "^3.4.13",
    "turbo": "^2.1.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
